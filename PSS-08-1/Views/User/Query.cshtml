@model List<PSS_08_1.Models.ViwModel.QueryViwModels>
@{
    ViewBag.Title = "Consulta";
    Layout = "~/views/shared/_LayoutApp.cshtml";
}


    <h2>Consulta Usuarios!!</h2>
    <div class="row">
        <br />
        <div class="col-md-12">
            <input type="button" class="btn btn-primary btn-lg "
                   onclick="document.location.href = '@Url.Content("~/")'"
                   value="Volver Atras" />
        </div>
        <br /> 
        <br />

        <div class="col-lg-12 ">
            <table class="table table-bordered">
                <tr>
                    <th style="background-color:#0094ff; color: white;">ID</th>
                    <th style="background-color:#0094ff; color: white;">Correo</th>
                    <th style="background-color:#0094ff; color: white;">Edad</th>
                    <th style="background-color:#0094ff; color: white;">Accion A Tomar</th>
                </tr>
                @foreach(var Objs in Model)
                {
                    <tr>
                        <td>@Objs._Id</td>
                        <td>@Objs._Email</td>
                        <td>@Objs._Edad</td>
                        <td>
                            <input type="button" class="btn btn-danger btn-lg "
                                   onclick="jsDelete(@Objs._Id);"
                                   value="Borrar" />

                            <input type="button" class="btn btn-default btn-lg "
                                   onclick=""
                                   value="..." />
                        </td>
                    </tr>
                }

            </table>
        </div>
    </div>